pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/DeepthiMogaparthi/maven_poc.git'
                
            }
        }    
        stage('clean') {
            steps {
                echo 'Building..'
                sh 'mvn -version'
                sh 'mvn clean'
                
            }    
        }
        stage('compile') {
            steps {
                echo 'compile'
                sh 'mvn compile'
            }
        }
        
    }
}
